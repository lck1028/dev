    "qs": "^6.9.4",
    "query-string": "^6.13.1",
    "react": "16.13.1",
    "react-device-detect": "^1.13.1",
    "react-dom": "16.13.1",
    "react-icons": "^3.10.0",
    "react-id-swiper": "^3.0.0",
    "react-moment": "^0.9.7",
    "react-redux": "^7.2.0",
